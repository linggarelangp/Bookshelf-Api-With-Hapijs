{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi":["error", "never"],
        "no-console":["off", "never"],
        "max-len":["off", "never"],
        "indent":"off",
        "padded-blocks":"off",
        "import/extensions":"off",
        "object-shorthand":"off"
        //"radix":["error", "never"]
    }
}
